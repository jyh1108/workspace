<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kh.test.user.model.dao.UserMapper">
	
	<resultMap id="User" type="user_rm">
		
		<proporty id="userNo" colrum="USER_NO" />
		<proporty result="userId" colrum="USER_ID" />
		<proporty result="userName" colrum="USER_NAME" />
		<proporty result="userAge" colrum="USER_AGE" />
	</resultMap>

	<select id="searchId" resultType="user_rm">
		SELECT * FROM TB_USER
		WHERE USER_ID = #{userId}
	</select>
</mapper>